
<div class="modal fade bd-example-modal-xl" tabindex="-1" id="tirage" role="dialog"
    aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dna" role="document" style="width:100%">
        <div class="modal-content text-center justify-content-center">
            <div class="modal-header">
                <h5 class="modal-title">tirage</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body text-center justify-content-center d-flex">
            {% for cat in categ2 %}
                <div class="ml-5" id="tirageCat">
                    <p>{{cat.name}}</p>
                    {# <p>{{cat.nameRand}}</p> #}
                </div>
            {% endfor %}
            </div>
            <div class="modal-footer">
                <button type="button" class="btn" data-dismiss="modal">Fermer</button>
            </div>

        </div>
    </div>
</div>

